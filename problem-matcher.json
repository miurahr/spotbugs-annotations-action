{
  "problemMatcher": [
    {
      "owner": "spotbugs",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^.*((?:H|M|L)\\s+(?:B|D|I|P|U|V)\\s+(?:[A-Z0-9_]+\\s+)?(?:[A-Za-z0-9]+)):\\s+(.*)\\s+At\\s+([A-Za-z0-9_]+\\.java):\\[line (\\d+)\\]$",
          "file": 3,
          "line": 4,
          "message": 2,
          "code": 1
        }
      ]
    }
  ]
}
