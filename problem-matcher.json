{
  "problemMatcher": [
    {
      "owner": "spotbugs",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^.*(H|M|L)\\s+((:?D|I|P|U|V)\\s+(:?[A-Z_]+)?(:?[A-Z][a-z]+[0-9]?)):\\s+(.*)\\s+At\\s+([A-Za-z0-9_]+\\.java):\\[line (\\d+)\\]$",
          "severity": 1,
          "file": 4,
          "line": 5,
          "message": 3,
          "code": 2
        }
      ]
    }
  ]
}
